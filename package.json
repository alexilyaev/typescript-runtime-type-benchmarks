{
  "name": "typescript-runtime-type-benchmarks",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rm -rf dist",
    "start": "vite-node src/main.ts",
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "lint-eslint": "eslint --max-warnings 0 '**/*.{js,cjs,ts,mts,tsx}'",
    "lint-eslint-debug": "DEBUG=eslint:cli-engine pnpm lint-eslint",
    "lint-eslint-summary": "pnpm lint-eslint -f summary",
    "lint-eslint-print-config": "eslint --print-config temp.ts",
    "lint-prettier": "prettier --check '**/*.{js,cjs,ts,mts,tsx,html,css,json,md}'",
    "lint": "pnpm lint-eslint && pnpm lint-prettier",
    "format-eslint": "eslint --fix '**/*.{js,cjs,ts,mts,tsx}'",
    "format-prettier": "prettier --write '**/*.{js,cjs,ts,mts,tsx,html,css,json,md}'",
    "format": "pnpm format-eslint && pnpm format-prettier",
    "type-check": "tsc --noEmit",
    "coverage": "vitest --coverage.enabled --coverage.provider=istanbul --coverage.all",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "test-bench": "vitest bench"
  },
  "dependencies": {
    "arktype": "2.1.20",
    "semver": "7.7.2",
    "ts-essentials": "10.0.4",
    "ts-extras": "0.13.0",
    "type-fest": "4.29.1",
    "valibot": "1.1.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@alexilyaev/browserslist-config": "0.1.3",
    "@alexilyaev/configs": "0.5.3",
    "@stylistic/eslint-plugin": "2.11.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/eslint": "9.6.1",
    "@types/node": "20",
    "@types/semver": "7.7.0",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "@vitejs/plugin-react": "4.6.0",
    "eslint": "8.57.0",
    "eslint-config-ai": "3.6.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-gitignore": "0.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unicorn": "56.0.1",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.4.1",
    "typescript": "~5.8.3",
    "vite": "7.0.0",
    "vite-node": "3.2.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "packageManager": "pnpm@9.12.1",
  "private": true,
  "keywords": [
    "typescript",
    "runtime",
    "type",
    "benchmark"
  ],
  "author": "Alex Ilyaev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexilyaev/typescript-runtime-type-benchmarks.git"
  },
  "bugs": {
    "url": "https://github.com/alexilyaev/typescript-runtime-type-benchmarks/issues"
  },
  "homepage": "https://github.com/alexilyaev/typescript-runtime-type-benchmarks#readme"
}
